{
  "name": "lancer-extension",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "background": "webpack --config background.webpack.config.js",
    "background-dev": "NODE_ENV=development webpack --config background.webpack.config.js",
    "compile": "yarn background && yarn content && cp manifest.json dist",
    "compile-dev": "yarn background-dev && yarn content",
    "content": "webpack --config content.webpack.config.js",
    "clean": "rm -rf dist && mkdir dist",
    "tar": "tar -zcvf lancer-extension.tar.gz dist"
  },
  "dependencies": {
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "axios": "1.2.3",
    "buffer": "6.0.3",
    "classnames": "2.3.2",
    "lodash": "4.17.21",
    "process": "0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "1.56.2",
    "sass-loader": "13.2.0",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@types/chrome": "0.0.208",
    "@types/lodash": "4.14.191",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/sass-loader": "8.0.3",
    "browserify-zlib": "^0.2.0",
    "buffer": "6.0.3",
    "crypto-browserify": "^3.12.0",
    "eslint": "^8.29.0",
    "eslint-plugin-react": "^7.31.11",
    "https-browserify": "^1.0.0",
    "os-browserify": "^0.3.0",
    "process": "0.11.10",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "5.0.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "tsc-alias": "1.8.2",
    "tsconfig-paths": "4.1.2",
    "tscpaths": "0.0.9",
    "typescript": "^4.6.4",
    "webpack-cli": "^5.0.1"
  }
}